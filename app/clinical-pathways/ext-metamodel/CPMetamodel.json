{
  "name": "Clinical Pathways",
  "uri": "http://some-company/schema/bpmn/cp",
  "prefix": "cp",
  "xml": {
    "tagAlias": "lowerCase"
  },
  "types": [
    {
      "name": "Process",
      "extends": [
        "bpmn:Process"
      ],
      "properties": [
        {
          "name": "evidenceIndicator",
          "default": "",
          "isAttr": true,
          "type": "EvidenceIndicator"
        }
      ]
    },
    {
      "name": "Task",
      "extends": [
        "bpmn:Task"
      ],
      "properties": [
        {
          "name": "evidenceIndicator",
          "default": "",
          "isAttr": true,
          "type": "EvidenceIndicator"
        },
        {
          "name": "objective",
          "default": "",
          "isAttr": true,
          "type": "Objective"
        },
        {
          "name": "qualityIndicator",
          "default": "",
          "isAttr": true,
          "isMany": true,
          "type": "QualityIndicator"
        }
      ]
    },
    {
      "name": "TherapyTask",
      "superClass": [
        "bpmn:Task"
      ]
    },
    {
      "name": "DiagnosisTask",
      "superClass": [
        "bpmn:Task"
      ]
    },
    {
      "name": "SupportingTask",
      "superClass": [
        "bpmn:Task"
      ]
    },
    {
      "name": "EducationTask",
      "superClass": [
        "bpmn:Task"
      ]
    },
    {
      "name": "HomeVisitTask",
      "superClass": [
        "bpmn:Task"
      ]
    },
    {
      "name": "MonitoringTask",
      "superClass": [
        "bpmn:Task"
      ]
    },
    {
      "name": "PhoneContactTask",
      "superClass": [
        "bpmn:Task"
      ]
    },
    /* Gateways */
    {
      "name": "Gateway",
      "extends": [
        "bpmn:Gateway"
      ],
      "properties": [
        {
          "name": "evidenceIndicator",
          "default": "",
          "isAttr": true,
          "type": "EvidenceIndicator"
        }
      ]
    },
    {
      "name": "SimultanParallelGateway",
      "superClass": [
        "bpmn:ParallelGateway"
      ]
    },
    {
      "name": "EvidenceBasedGateway",
      "superClass": [
        "bpmn:ComplexGateway"
      ],
      "properties": [
        {
          "name": "dmn",
          "default": "",
          "isAttr": true,
          "type": "String"
        }
      ]
    },
    /* Properties */
    {
      "name": "EvidenceIndicator",
      "extends": [
        "bpmn:Property"
      ],
      "properties": [
        {
          "name": "evidenceLevel",
          "isAttr": true,
          "type": "EvidenceLevel"
        }
      ]
    },
    {
      "name": "QualityIndicator",
      "extends": [
        "bpmn:Property"
      ],
      "properties": [
        {
          "name": "quality",
          "isAttr": true,
          "type": "Integer"
        }
      ]
    },
    {
      "name": "Objective",
      "extends": [
        "bpmn:Property"
      ],
      "properties": [
        {
          "name": "objective",
          "isAttr": true,
          "type": "String"
        }
      ]
    },
    /* CPG Reference */
    {
      "name": "CPGReference",
      "properties": [
        {
          "name": "pageFrom",
          "isAttr": true,
          "type": "Integer"
        },
        {
          "name": "pageTo",
          "isAttr": true,
          "type": "Integer"
        },
        {
          "name": "paragraphFrom",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "paragraphTo",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "url",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "cpgName",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "evidenceIndicators",
          "isMany": true,
          "type": "EvidenceIndicator"
        },
        {
          "name": "document",
          "isAttr": true,
          "type": "bpmn:DataObject",
          "isReference": true
        },
        {
          "name": "referredStatements",
          "isMany": true,
          "type": "ClinicalStatement",
          "isReference": true
        }
      ]
    },
    /* Clinical Statement */
    {
      "name": "ClinicalStatement",
      "isAbstract": true,
      "superClass": [
        "bpmn:DataObject"
      ],
      "properties": [
        {
          "name": "statementContent",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "guidelineReference",
          "isAttr": true,
          "type": "CPGReference",
          "isReference": true
        },
        {
          "name": "section",
          "isAttr": true,
          "type": "Segment",
          "isReference": true
        },
        {
          "name": "organizer",
          "isAttr": true,
          "type": "Organizer",
          "isReference": true
        }
      ]
    },
    {
      "name": "Observation",
      "superClass": [
        "ClinicalStatement"
      ]
    },
    {
      "name": "Medication",
      "superClass": [
        "ClinicalStatement"
      ]
    },
    {
      "name": "Action",
      "superClass": [
        "ClinicalStatement"
      ]
    },
    {
      "name": "Meeting",
      "superClass": [
        "ClinicalStatement"
      ]
    },
    {
      "name": "Care",
      "superClass": [
        "ClinicalStatement"
      ]
    },
    {
      "name": "MultimediaObject",
      "superClass": [
        "ClinicalStatement"
      ]
    },
    {
      "name": "ObjectiveArea",
      "superClass": [
        "ClinicalStatement"
      ]
    },
    {
      "name": "Procedure",
      "superClass": [
        "ClinicalStatement"
      ]
    },
    /* Organizer */
    {
      "name": "Organizer",
      "superClass": [
        "bpmn:DataObject",
        "bpmn:FlowElementsContainer"
      ]
    },
    /* Segment */
    {
      "name": "Segment",
      "superClass": [
        "bpmn:DataObject",
        "bpmn:FlowElementsContainer"
      ],
      "properties": [
        {
          "name": "title",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "code",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "text",
          "isAttr": true,
          "type": "String"
        }
      ]
    },
    /* ClinicalDocument */
    {
      "name": "ClinicalDocument",
      "superClass": [
        "bpmn:DataObject"
      ],
      "properties": [
        {
          "name": "serializedContent",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "relatedCaseChart",
          "isMany": true,
          "type": "CaseChart",
          "isReference": true
        }
      ]
    },
    /* UnstructuredDocument */
    {
      "name": "UnstructuredDocument",
      "superClass": [
        "ClinicalDocument"
      ]
    },
    {
      "name": "VideoDocument",
      "superClass": [
        "UnstructuredDocument"
      ]
    },
    {
      "name": "SignalingDocument",
      "superClass": [
        "UnstructuredDocument"
      ]
    },
    {
      "name": "TextDocument",
      "superClass": [
        "UnstructuredDocument"
      ]
    },
    {
      "name": "ImageDocument",
      "superClass": [
        "UnstructuredDocument"
      ]
    },
    /* StructuredDocument */
    {
      "name": "StructuredDocument",
      "superClass": [
        "ClinicalDocument",
        "bpmn:FlowElementsContainer"
      ],
      "properties": [
        {
          "name": "segment",
          "isMany": true,
          "type": "Segment",
          "isReference": true
        }
      ]
    },
    /* CaseChart */
    {
      "name": "CaseChart",
      "superClass": [
        "bpmn:DataObject",
        "bpmn:FlowElementsContainer"
      ],
      "properties": [
        {
          "name": "document",
          "isMany": true,
          "type": "ClinicalDocument",
          "isReference": true
        }
      ]
    },
    /* Resources */
    {
      "name": "CPResource",
      "superClass": [
          "bpmn:FlowElement"
      ],
      "extends": [
        "bpmn:Resource"
      ],
      "properties": [
        {
          "name": "resourceBundles",
          "isMany": true,
          "type": "ResourceBundle",
          "isReference": true
        }
      ]
    },
    {
      "name": "ResourceBundle",
      "superClass": [
        "bpmn:DataObject",
        "bpmn:FlowElementsContainer"
      ],
      "extends": [
        "bpmn:Resource"
      ]
    },
    {
      "name": "HumanResource",
      "superClass": [
        "CPResource"
      ],
      "properties": [
        {
          "name": "organizer",
          "isMany": true,
          "type": "Organizer",
          "isReference": true
        }
      ]
    },
    {
      "name": "ConsumptionResource",
      "superClass": [
        "CPResource"
      ]
    },
    {
      "name": "Equipment",
      "superClass": [
        "CPResource"
      ]
    },
    {
      "name": "Medicine",
      "superClass": [
        "CPResource"
      ]
    },
    {
      "name": "TransportationEquipment",
      "superClass": [
        "CPResource"
      ]
    },
    {
      "name": "Room",
      "superClass": [
        "CPResource"
      ]
    },
    {
      "name": "Auxiliaries",
      "superClass": [
        "CPResource"
      ]
    },
    /* RELATIONS */
    /* Relation between Clinical Statements */
    {
      "name": "StatementRelation",
      "superClass": [
        "bpmn:Association"
      ],
      "properties": [
        {
          "name": "name",
          "type": "String",
          "isAttr": true
        }
      ]
    },
    /* Relation between BPMN-Elements and Case Charts */
    {
      "name": "CaseChartAssociation",
      "superClass": [
        "bpmn:Association"
      ],
      "properties": [
        {
          "name": "name",
          "type": "String",
          "isAttr": true
        }
      ]
    },

    /* Relation between two CPResources / ResourceBundles */
    {
      "name": "ResourceRelation",
      "superClass": [
        "bpmn:Association"
      ]
    },
    /* Relation between BPMN-Elements and CPResource */
    {
      "name": "ResourceAssociation",
      "superClass": [
        "bpmn:Association"
      ]
    },
    {
      "name": "AssociationAnnotation",
      "superClass": [
        "bpmn:TextAnnotation"
      ]
    }

  ],
  "enumerations": [
    {
      "name": "EvidenceLevel",
      "literalValues": [
        {
          "name": "A"
        },
        {
          "name": "B"
        },
        {
          "name": "C"
        },
        {
          "name": "D"
        },
        {
          "name": "E"
        },
        {
          "name": "F"
        }
      ]
    }
  ]
}